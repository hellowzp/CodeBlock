CC = gcc
CFLAGS = -Wall -c
LFLAGS = -Wall

a.out:gateway.o D_list.o
	$(CC) -shared gateway.o D_list.o -o -soname liblist.so	               //s<

D_list.exe:D_list.o
	$(CC) $(LFLAGS) D_list.o -o D_list.exe           //s<

gateway.o:gateway.c gateway.h
	$(CC) $(CFLAGS) gateway.c
	
D_list.o:D_list.c D_list.h
	$(CC) $(CFLAGS) D_list.c

                                              //make simulator ;make gateway
clean:
rm -f *.o $(EXE) *~
