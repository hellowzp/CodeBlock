#include "healthpackage.h"
#include "mainwindow.h"

HealthPackage::HealthPackage(): Tile(0, 0, 0){

    this->setPixmap(QPixmap(":/world/medical-suitecase-icon.png").scaledToHeight(15).scaledToWidth(15));
}

void HealthPackage::MoveToPoint(int x, int y) {

    this->setXPos(x);
    this->setYPos(y);
    this->setPos(this->getXPos() * SCALE_FACTOR, this->getYPos() * SCALE_FACTOR);
}

